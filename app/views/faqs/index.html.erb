<p id="notice"><%= notice %></p>

<div class="answers">
  <header>
    <img src="assets/me.JPG">
  </header>



  <h1>Listing Faqs</h1>

  <table>
    <thead>
      <tr>
        <th>Question</th>
        <th>Answer</th>
        <th>More-info</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @faqs.each do |faq| %>
        <tr>
          <td><%= faq.question %></td>
          <td><%= faq.answer %></td>
          <td><%= faq.more-info %></td>
          <td><%= link_to 'Show', faq %></td>
          <td><%= link_to 'Edit', edit_faq_path(faq) %></td>
          <td><%= link_to 'Destroy', faq, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'Back to Bacon', root_path %>
</div>
